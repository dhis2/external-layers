{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Google Earth Engine layer for DHIS2 Maps",
  "description": "",
  "type": "object",
  "properties": {
    "datasetId": {
      "type": "string",
      "minLength": 1
    },
    "name": {
      "type": "string",
      "minLength": 1
    },
    "description": {
      "type": "string"
    },
    "band": {
      "type": "string"
    },
    "aggregations": {
      "type": "array"
    },
    "defaultAggregations": {
      "type": "array"
    },
    "methods": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["name", "arguments"],
        "properties": {
          "name": {
            "type": "string"
          },
          "arguments": {
            "type": "array"
          }
        }
      }
    },
    "filters": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["type", "name", "value"],
        "properties": {
          "type": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "value": {}
        }
      }
    },
    "classes": {
      "type": "array",
      "uniqueItems": true,
      "items": {
        "type": "object",
        "required": ["value", "color", "name"],
        "properties": {
          "value": {
            "type": "integer"
          },
          "color": {
            "type": "string"
          },
          "name": {
            "type": "string"
          }
        }
      }
    }
  },
  "required": ["datasetId", "name", "description"],
  "additionalProperties": false
}
